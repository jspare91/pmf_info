@model pmf_2.Models.project
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>C O Log</h1>
</div>
<div>
    @using (Html.BeginForm("Index", "co_log", null, FormMethod.Post, null))
    {
        <table>
            <tr>
                <td width="200">
                    @Html.LabelFor(model => model.project_name, "project_name", htmlAttributes: new { @class = "control-label col-md-2" })
                </td>
                <td>
                    @Html.DropDownList("project_Id", null, htmlAttributes: new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.project_Id, "", new { @class = "text-danger" })
                </td>
                <td width="200">
                    <input id="btnRefresh" type="submit" value="Submit" class="btn-default" style="border:none; color:#428bca; fill-opacity:inherit" />
                </td>
            </tr>
            <tr></tr>
            <tr></tr>
            <tr></tr>
            <tr></tr>
        </table>
    <div class="col-md-4">
        @Html.ActionLink("Change Order Log", "Index", "co_log", "null", "null")
        <br />
        @Html.ActionLink("Project Manager", "Index", "proj_man", "null", "null")
        <br />
        @Html.ActionLink("Projects", "Index", "projects", "null", "null")
    </div>
    }
</div>
