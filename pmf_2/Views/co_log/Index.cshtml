@model IEnumerable<pmf_2.Models.co_log>

@{
    ViewBag.Title = "Index";
}

<h2> @ViewBag.project_name</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div class="form-group">
    <input type="hidden" id="project_Id" name="project_Id" value="@ViewBag.project_Id" />
    </div>
</div>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.co_num)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.proc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.n_proc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.gc_co)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.owed)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.notes)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.project.project_name)
        </th>*@
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.co_num)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.proc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.n_proc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.gc_co)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.owed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.notes)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.project.project_name)
        </td>*@
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.co_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.co_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.co_Id })
        </td>
    </tr>
}

</table>
