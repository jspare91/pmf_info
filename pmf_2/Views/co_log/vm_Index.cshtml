@model IEnumerable<pmf_2.Models.co_log>

@{
    ViewBag.Title = "vm_Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
