        </th>
        <th>
            @Html.DisplayNameFor(model => model.proc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.n_proc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.gc_co)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.owed)
        </th>
        <th>
