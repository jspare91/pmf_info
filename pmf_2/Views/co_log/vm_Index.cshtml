            <td>
                @Html.DisplayFor(modelItem => item.gc_co)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.owed)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.notes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.project.project_name)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.co_Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.co_Id }) |
